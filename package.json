{
  "name": "@circlesac/tsed-logger-cloudwatch-logs",
  "version": "0.0.13",
  "type": "module",
  "scripts": {
    "barrels": "barrels",
    "build": "rimraf dist && barrels && npx @circlesac/tscc --include src",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:biome",
    "lint:eslint": "npx eslint . --fix",
    "lint:prettier": "npx prettier '**/*.{ts,tsx,js,jsx,json,md,cjs,mjs,mts,yml,yaml}' --write",
    "lint:biome": "npx biome check --write",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.777.0",
    "@tszen/trycatch": "^0.1.0"
  },
  "devDependencies": {
    "@circlesac/tscc": "^0.0.7",
    "@piondev/shared-configs": "^1.0.35",
    "@tsed/barrels": "^6.2.1",
    "@tsed/logger": "^7.0.2",
    "@types/node": "^22.13.14",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@tsed/logger": "^7.0.2"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@1.22.19"
}
